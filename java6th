import java.io.File;
import java.io.FileInputStream;
import java.io.FileOutputStream;
import java.io.FileWriter;
import java.io.IOException;
import java.io.InputStream;
import java.io.OutputStream;

public class Filei {

	public static void main(String[] args) throws IOException {
		// TODO Auto-generated method stub
	try {
		File obj=new File("C:\\Users\\Personal\\Documents\\SH\\Filename.txt");
		FileWriter mw=new FileWriter(â€œC:\\Users\\Personal\\Documents\\SH\\Filename.txt");
		mw.write("file is created");

		mw.close();
		File obj1=new File("C:\\Users\\Personal\\Documents\\SH\\Filename1.txt");
		
	InputStream fis=new FileInputStream(obj);
	OutputStream fos=new FileOutputStream(obj1);
	int i;
	while((i=fis.read())!=-1)
	{
		fos.write(i);
	}fis.close();
	fos.close();
	
		
		
	}catch(Exception e)
	{
		System.out.println(e);
	}
	}

}
